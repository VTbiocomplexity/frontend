{
  "extends": ["./node_modules/aurelia-tools/.eslintrc.json", "plugin:react/recommended", "airbnb"],
  "rules": {
    "no-console": [0, "never"],
    "no-alert": [0, "never"],
     "comma-dangle": ["error", "never"],
     "no-undef": [0, "never"],
     "react/jsx-filename-extension": [0, "never"],
     "func-names": [0, "never"],
     "max-len": ["error", { "code": 150 }],
     "class-methods-use-this": [0, "never"],
     "no-underscore-dangle": [0, "never"]
  }
}
